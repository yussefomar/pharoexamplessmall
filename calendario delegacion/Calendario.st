TestCase subclass: #CalendarioTest	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'Calendario-Tests'!!CalendarioTest methodsFor: 'as yet unclassified' stamp: '92963 3/30/2017 11:34'!test24deSeptEsFeriadoSoloEn2012	| calendario veinticuatroSept2012 veinticuatroSept2015 |		calendario := Calendario new.	veinticuatroSept2012 := Date year: 2012 month:9 day: 24.	veinticuatroSept2015 := Date year: 2015 month:9 day: 24.	self shouldnt: (calendario esFeriado: veinticuatroSept2012).		calendario agregarFeriadoPuntualEnAnio: 2012 yMes: 9 yDia: 24.			self should: (calendario esFeriado: veinticuatroSept2012).	self shouldnt: (calendario esFeriado: veinticuatroSept2015).! !!CalendarioTest methodsFor: 'as yet unclassified' stamp: 'asdasdasdas 3/26/2017 20:23'!test24deMarEsFeriadoDesde2006	| calendario veinticuatroMar2006 veinticuatroMar2008 |		calendario := Calendario new.	veinticuatroMar2006 := Date year: 2006 month: 3 day: 24.	veinticuatroMar2008 := Date year: 2006 month: 3 day: 24.	self shouldnt: (calendario esFeriado: veinticuatroMar2006).		calendario agregarFeriadoDesdeAnio: 2006 enMes: 3 yDia: 24.			self should: (calendario esFeriado: veinticuatroMar2006).	self should: (calendario esFeriado: veinticuatroMar2008).! !!CalendarioTest methodsFor: 'as yet unclassified' stamp: '92963 3/30/2017 14:22'!testTodosLosDomingosonFeriadosPartir2015| unCalendario unDomingo otroDomingo |unCalendario:=Calendario new.unDomingo:= Date year: 2012 month:5  day:7. otroDomingo:= Date year: 2013month:5  day:7.unCalendario agregarFeriadoSemanalDesdeAnio:2012 mes:5 yDia:7.unCalendario agregarFeriadoSemanalDesdeAnio:2013  mes:5 yDia:7.self assert:(unCalendario esFeriado: unDomingo ).self assert:(unCalendario esFeriado: otroDomingo ).! !!CalendarioTest methodsFor: 'as yet unclassified' stamp: '92963 3/30/2017 14:02'!testTodosLosDomingoSonFeriados| unCalendario unDomingo otroDomingo |unCalendario:=Calendario new.unDomingo:= Date year: 2012 month:5  day:7. otroDomingo:= Date year: 2013month:5  day:7.unCalendario agregarFeriadoSemanalEnAnio:2012 mes:5 yDia:7.unCalendario agregarFeriadoSemanalEnAnio:2013  mes:5 yDia:7.self assert:(unCalendario esFeriado: unDomingo ).self assert:(unCalendario esFeriado: otroDomingo ).! !!CalendarioTest methodsFor: 'as yet unclassified' stamp: 'asdasdasdas 3/23/2017 16:57'!testNavidadEsFeriado	| calendario navidad2015 navidad2018 |		calendario := Calendario new.	navidad2015 := Date year: 2015 month:12 day: 25.	navidad2018 := Date year: 2018 month:12 day: 25.	self shouldnt: (calendario esFeriado: navidad2015).		calendario agregarFeriadoAnualEnMes: 12 yDia: 25.			self should: (calendario esFeriado: navidad2015).	self should: (calendario esFeriado: navidad2018).! !Object subclass: #Calendario	instanceVariableNames: 'listaDeFeriados'	classVariableNames: ''	poolDictionaries: ''	category: 'Calendario'!!Calendario methodsFor: 'as yet unclassified' stamp: '92963 3/30/2017 14:04'!agregarFeriadoSemanalEnAnio: unAnio mes: unMes yDia: unDia 	| unFeriado |unFeriado:=FeriadoSemanal crearConDiaDeSemana:unDia.listaDeFeriados add:unFeriado. ! !!Calendario methodsFor: 'as yet unclassified' stamp: 'asdasdasdas 3/23/2017 18:08'!esFeriado: unaFecha	listaDeFeriados do: [ :feriado |		(feriado coincideCon: unaFecha) ifTrue: [ ^ true ].		].		^ false.! !!Calendario methodsFor: 'as yet unclassified' stamp: 'asdasdasdas 3/25/2017 10:31'!agregarFeriadoPuntualEnAnio: unAnio yMes: unMes yDia: unDia	| feriadoPuntual |		feriadoPuntual := FeriadoDesdeAnio crearConAnio: unAnio yMes: unMes yDia: unDia.		listaDeFeriados add: feriadoPuntual.! !!Calendario methodsFor: 'as yet unclassified' stamp: 'asdasdasdas 3/26/2017 20:26'!agregarFeriadoDesdeAnio: unAnio enMes: unMes yDia: unDia	| feriado |		feriado := FeriadoDesdeAnio crearConAnio: unAnio yMes: unMes yDia: unDia.	listaDeFeriados add: feriado.	! !!Calendario methodsFor: 'as yet unclassified' stamp: 'asdasdasdas 3/23/2017 18:11'!agregarFeriadoAnualEnMes: unMes yDia: unDia	| feriadoAnual |		feriadoAnual := FeriadoAnual crearConMes: unMes yDia: unDia.		listaDeFeriados add: feriadoAnual.	! !!Calendario methodsFor: 'as yet unclassified' stamp: 'asdasdasdas 3/23/2017 18:03'!initialize	listaDeFeriados := OrderedCollection new.	! !!Calendario methodsFor: 'as yet unclassified' stamp: '92963 3/30/2017 14:29'!agregarFeriadoSemanalDesdeAnio: unAnio mes: anInteger2 yDia: unDia | unFeriado |unFeriado:=FeriadoSemanalDesdeAnio crearConAnio:unAnio yDiaDeSemana:unDia.listaDeFeriados add:unFeriado. ! !Object subclass: #FeriadoAnual	instanceVariableNames: 'mes dia'	classVariableNames: ''	poolDictionaries: ''	category: 'Calendario'!!FeriadoAnual methodsFor: 'as yet unclassified' stamp: 'asdasdasdas 3/23/2017 18:15'!coincideCon: unaFecha	^ ((mes = unaFecha monthIndex) & (dia = unaFecha dayOfMonth))! !!FeriadoAnual methodsFor: 'as yet unclassified' stamp: 'asdasdasdas 3/23/2017 17:58'!setDia: unDia	dia := unDia.	! !!FeriadoAnual methodsFor: 'as yet unclassified' stamp: 'asdasdasdas 3/23/2017 17:58'!setMes: unMes	mes := unMes.	! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!FeriadoAnual class	instanceVariableNames: ''!!FeriadoAnual class methodsFor: 'as yet unclassified' stamp: 'asdasdasdas 3/23/2017 17:57'!crearConMes: unMes yDia: unDia	| feriadoAnual |		feriadoAnual := FeriadoAnual new.	feriadoAnual setMes: unMes.	feriadoAnual setDia: unDia.	^ feriadoAnual.! !Object subclass: #FeriadoDesdeAnio	instanceVariableNames: 'anio feriado'	classVariableNames: ''	poolDictionaries: ''	category: 'Calendario'!!FeriadoDesdeAnio methodsFor: 'as yet unclassified' stamp: 'asdasdasdas 3/25/2017 10:34'!coincideCon: unaFecha 	^ ((anio >= unaFecha year) & (feriado coincideCon: unaFecha)).	! !!FeriadoDesdeAnio methodsFor: 'as yet unclassified' stamp: 'asdasdasdas 3/25/2017 10:31'!setAnioDesde: unAnio 	anio := unAnio.! !!FeriadoDesdeAnio methodsFor: 'as yet unclassified' stamp: 'asdasdasdas 3/25/2017 10:34'!setFeriado: unFeriadoAnual 		feriado := unFeriadoAnual.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!FeriadoDesdeAnio class	instanceVariableNames: ''!!FeriadoDesdeAnio class methodsFor: 'as yet unclassified' stamp: '92963 4/2/2017 21:29'!crearConAnio: unAnio yFeriado: unFeriado	| feriadoDesde |		feriadoDesde := FeriadoDesdeAnio new.	feriadoDesde setAnioDesde: unAnio.	feriadoDesde setFeriado: unFeriado.	^ feriadoDesde.! !!FeriadoDesdeAnio class methodsFor: 'as yet unclassified' stamp: 'asdasdasdas 3/25/2017 10:44'!crearConAnio: unAnio yMes: unMes yDia: unDia 	| feriadoDesde feriadoAnual |		feriadoDesde := FeriadoDesdeAnio new.	feriadoDesde setAnioDesde: unAnio.	feriadoAnual := FeriadoAnual crearConMes: unMes yDia: unDia. 	feriadoDesde setFeriado: feriadoAnual.	^ feriadoDesde.! !Object subclass: #FeriadoHastaAnio	instanceVariableNames: 'anio feriado'	classVariableNames: ''	poolDictionaries: ''	category: 'Calendario'!!FeriadoHastaAnio methodsFor: 'as yet unclassified' stamp: 'asdasdasdas 3/25/2017 10:34'!coincideCon: unaFecha 	^ ((anio <= unaFecha year) & (feriado coincideCon: unaFecha)).	! !!FeriadoHastaAnio methodsFor: 'as yet unclassified' stamp: 'asdasdasdas 3/25/2017 10:33'!setAnioHasta: unAnio 	anio := unAnio.! !!FeriadoHastaAnio methodsFor: 'as yet unclassified' stamp: 'asdasdasdas 3/25/2017 10:34'!setFeriado: unFeriadoAnual 		feriado := unFeriadoAnual.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!FeriadoHastaAnio class	instanceVariableNames: ''!!FeriadoHastaAnio class methodsFor: 'as yet unclassified' stamp: '92963 3/30/2017 12:51'!crearConAnio: unAnio yFeriado: unFeriado	| feriadoHasta |		feriadoHasta := FeriadoHastaAnio  new.	feriadoHasta setAnioHasta: unAnio.	feriadoHasta setFeriado: unFeriado.	^ feriadoHasta.! !!FeriadoHastaAnio class methodsFor: 'as yet unclassified' stamp: '92963 3/30/2017 12:51'!crearConAnio: unAnio yMes: unMes yDia: unDia 	| feriadoHasta feriadoAnual |		feriadoHasta := FeriadoHastaAnio  new.	feriadoHasta setAnioHasta: unAnio.	feriadoAnual := FeriadoAnual crearConMes: unMes yDia: unDia. 	feriadoHasta setFeriado: feriadoAnual.	^ feriadoHasta.! !Object subclass: #FeriadoPuntual	instanceVariableNames: 'anio feriado'	classVariableNames: ''	poolDictionaries: ''	category: 'Calendario'!"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!FeriadoPuntual class	instanceVariableNames: ''!!FeriadoPuntual class methodsFor: 'as yet unclassified' stamp: 'asdasdasdas 3/25/2017 10:44'!crearConAnio: unAnio yMes: unMes yDia: unDia	| feriadoAnual feriadoDesde feriadoHasta |		"un feriado puntual es un feriado anual que va desde y hasta el mismo anio"		feriadoAnual := FeriadoAnual crearConMes: unMes yDia: unDia.	feriadoDesde := FeriadoDesdeAnio crearConAnio: unAnio yFeriado: feriadoAnual.	feriadoHasta := FeriadoHastaAnio crearConAnio: unAnio yFeriado: feriadoDesde.	^ feriadoHasta. ! !Object subclass: #FeriadoSemanal	instanceVariableNames: 'dia'	classVariableNames: ''	poolDictionaries: ''	category: 'Calendario'!!FeriadoSemanal methodsFor: 'as yet unclassified' stamp: '92963 3/30/2017 14:08'!setDiaSemana: unDia.dia:=unDia.! !!FeriadoSemanal methodsFor: 'as yet unclassified' stamp: '92963 3/30/2017 14:13'!coincideCon: unFecha| coincide |coincide:=false.(unFecha dayOfMonth= dia)ifTrue: [ coincide:=true.].^coincide ! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!FeriadoSemanal class	instanceVariableNames: ''!!FeriadoSemanal class methodsFor: 'as yet unclassified' stamp: '92963 3/30/2017 14:07'!crearConDiaDeSemana: unDia| unFeriadoSemanal |unFeriadoSemanal:= FeriadoSemanal new.unFeriadoSemanal setDiaSemana:unDia.^unFeriadoSemanal ! !Object subclass: #FeriadoSemanalDesdeAnio	instanceVariableNames: ''	classVariabl